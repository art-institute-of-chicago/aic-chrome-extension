{
    "manifest_version": 2,
    "name": "Art Institute of Chicago: Art Tab",
    "description": "Explore the Art Institute of Chicago's Collection",
    "version": "1.0",
    "author": "AIC",
    "browser_action": {
        "default_icon": "assets/aicLogo.png",
        "default_title": "Art Institute of Chicago"
    },
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net https://ssl.google-analytics.com; object-src 'self'",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "activeTab"
    ]
}
